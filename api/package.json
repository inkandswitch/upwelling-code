{
  "name": "api",
  "version": "1.0.2",
  "description": "backend for upwell",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "ts-mocha -p tsconfig.json --type-check test/*.ts",
    "watch": "tsc --watch",
    "build": "tsc",
    "release": "npm run build:cjs && npm run build:esm && npm run build:umd && npm run build:types",
    "build:cjs": "node tools/cleanup cjs && tsc -p config/tsconfig.cjs.json",
    "build:esm": "node tools/cleanup esm && tsc -p config/tsconfig.esm.json",
    "build:umd": "node tools/cleanup umd && webpack --config config/webpack.config.js",
    "build:types": "node tools/cleanup types && tsc -p config/tsconfig.types.json"
  },
  "files": [
    "dist"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
    "@types/concat-stream": "^2.0.0",
    "@types/debug": "^4.1.7",
    "@types/diff": "^5.0.2",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.14",
    "buffer": "^5.7.1",
    "constants-browserify": "^1.0.0",
    "mocha": "9.1.3",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "ts-loader": "^9.2.6",
    "util": "^0.12.4",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@types/chai": "^4.3.0",
    "@types/tar-stream": "^2.2.2",
    "automerge-wasm-pack": "^0.0.20",
    "chai": "^4.3.6",
    "concat-stream": "^2.0.0",
    "debug": "^4.3.3",
    "nanoid": "^3.2.0",
    "random-desserts": "^0.2.2",
    "sializer": "^0.2.0",
    "stream-browserify": "^3.0.0",
    "tar-stream": "^2.2.0",
    "ts-mocha": "^9.0.2",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.5",
    "uuid": "^8.3.2"
  }
}
